<%- include('./partials/head') %>
<%- include('./partials/navigation') %>
<main>
    <section class="hero-banner"></section>
    <div class="header-container">
        <div>
            <h1>Projecten.</h1>
            <p>Voeg partners van Vervoerregio Amsterdam toen aan de projecten lijst.</p>
            <svg class="cubeimage" viewBox="0 0 34 31" fill="none" xmlns="http://www.w3.org/2000/svg%22%3E">
                <rect x="11.5" y="8.5" width="22" height="22" stroke="#E30059"/>
            <rect x="0.5" y="0.5" width="22" height="22" fill="white" stroke="#E30059" stroke-dasharray="2 2"/>
            </svg>
        </div>
    </div>
    <div class="partner-container">
        <% partnerData.urls.forEach(url=> { %>
            <div class="partner-cube">
                <div>
                    <h2><%= url.website.titel %></h2>
                    <a class="websites__link" target="_blank" href="<%= url.url %>"><%= url.url %></a>
                </div>
                <div class="website__urls">
                    <a class="website__icon" href="/check?id=<%= url.id %>">X</a>
                </div>
            </div>
            <%})%>
    </div>
    
</main>
<%- include('./partials/foot') %>